msgstr "ERROR: no fue posible leer el contenido del archivo \"%s\"."

#: ../src/webappmenu-setup.py:29
msgid "Add profile"
msgstr "Adjuntar un perfil"

#: ../src/webappmenu-setup.py:30
msgid "Use the default profile"
msgstr "Utilizar el perfil predeterminado"


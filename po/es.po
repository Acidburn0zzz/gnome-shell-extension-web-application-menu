msgid "use data from the json file for reading and writing"
msgstr "utilizar los datos del archivo json para leer y escribir"

#: ../src/webappmenu-setup.py:65
#, python-format
msgid ""
"WARNING: file \"%s\" not found!\n"
"Options initialized to their default values."
msgstr ""
"ATENCIÓN: ¡archivo \"%s\" no encontrado!\n"
"Opciones inicializadas a sus valores predeterminados."

#: ../src/webappmenu-setup.py:67
#, python-format
msgid ""
"Error writing to file \"%s\":\n"
"%s"
msgstr ""
"Error al escribir el archivo \"%s\":\n"
"%s"

#: ../src/webappmenu-setup.py:68
#, python-format
msgid ""
"WARNING: could not read file \"%s\"!\n"
"Options initialized to their default values."
msgstr ""
"ATENCIÓN: no fue posible leer el archivo \"%s\"!\n"
"Opciones inicializadas a sus valores predeterminados."


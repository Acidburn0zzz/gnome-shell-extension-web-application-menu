#: ../src/extension.js:51
msgid "Browse your Web Applications"
msgstr "Consultar sus aplicaciones"

#: ../src/extension.js:52
msgid "Advanced settings"
msgstr "Opciones avanzadas"

#: ../src/extension.js:55
msgid "Configuration file changed!!!"
msgstr "¡El archivo de configuración ha cambiado!"

#: ../src/extension.js:56
#, c-format
msgid "WARNING: file \"%s\" does not exist."
msgstr "ATENCIÓN: el archivo \"%s\" no existe."


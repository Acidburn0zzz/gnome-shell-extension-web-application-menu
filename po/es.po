#: ../src/webappmenu-setup.py:59
#, python-format
msgid ""
"WARNING: file \"%s\" is badly formatted.\n"
"Options initialized to their default values."
msgstr ""
"ATENCIÓN: archivo \"%s\" con formato incorrecto.\n"
"Opciones inicializadas a sus valores predeterminados."

#: ../src/webappmenu-setup.py:61
msgid "The base path is not a directory!"
msgstr "¡La ruta de base no es un directorio!"

#: ../src/webappmenu-setup.py:62
msgid "Could not create the base directory!"
msgstr "¡No fue posible crear el directorio de base!"

#: ../src/webappmenu-setup.py:63
#, python-format
msgid ""
"Problems detecting the following entries:\n"
"%s"
msgstr ""
"Problemas de analización de las siguientes entradas:\n"
"%s"

